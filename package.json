{
  "name": "msets",
  "version": "1.0.0",
  "description": "A web application for musician to generate sounds, add effects and share them.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-run-all --silent --parallel dev:*",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon server/index.js",
    "db:import": "source .env && psql $DATABASE_URL < database/dump.sql",
    "db:export": "source .env && pg_dump -c --if-exists -O $DATABASE_URL > database/dump.sql",
    "build": "webpack --mode=production",
    "start": "NODE_ENV=production node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashkankardan/msets.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashkankardan/msets/issues"
  },
  "homepage": "https://github.com/ashkankardan/msets#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "pg": "^8.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
